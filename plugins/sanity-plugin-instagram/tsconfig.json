{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "es5"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "commonjs",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "outDir": "dist",
    "downlevelIteration": true
  },
  "include": ["*.d.ts", "./src"],
  "exclude": ["node_modules"]
}
