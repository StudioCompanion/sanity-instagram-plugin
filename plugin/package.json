{
  "name": "sanity-plugin-instagram",
  "version": "0.0.1",
  "description": "Sanity plugin to pull in and curate your instagram content",
  "author": "Companion Studio Ltd",
  "license": "MIT",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "types": "./lib/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/cjs/index.js",
      "default": "./lib/esm/index.js"
    }
  },
  "files": [
    "src",
    "lib",
    "v2-incompatible.js",
    "sanity.json"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "prebuild": "npm run clean && plugin-kit verify-package --silent",
    "build": "parcel build --no-cache",
    "dev": "parcel watch",
    "watch": "parcel watch",
    "link-watch": "plugin-kit link-watch",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/StudioCompanion/sanity-instagram-plugin.git"
  },
  "dependencies": {
    "@sanity/client": "^3.3.2",
    "@sanity/icons": "^1.3.0",
    "@sanity/incompatible-plugin": "^0.0.1-studio-v3.1",
    "@sanity/ui": "^0.37.12",
    "@xstate/react": "^3.0.0",
    "axios": "^0.27.2",
    "groq": "^2.29.3",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "styled-components": "^5.3.5",
    "xstate": "^4.32.1",
    "zod": "^3.17.3",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@sanity/plugin-kit": "^0.0.1-studio-v3.4",
    "@types/styled-components": "^5.1.25",
    "parcel": "^2.6.0",
    "react": "^17.0.0 || ^18.0.0",
    "rimraf": "^3.0.2",
    "sanity": "2.29.5-purple-unicorn.856"
  },
  "peerDependencies": {
    "react": "^17.0.0 || ^18.0.0",
    "sanity": "*"
  },
  "bugs": {
    "url": "https://github.com/StudioCompanion/sanity-instagram-plugin/issues"
  },
  "homepage": "https://github.com/StudioCompanion/sanity-instagram-plugin#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "sanityPlugin": {
    "verifyPackage": {
      "tsconfig": false
    }
  }
}
